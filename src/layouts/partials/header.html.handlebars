            <!-- </div> -->
            <h1>Inclusive Design Guidelines</h1>
        </a>

        {{! Show the menu for mobile screens }}
        <nav class="right show-for-small-only docs-template-nav-mobile">
            <ul id="menu">
                <li class="docs-template-dropDown-header"><span class="docs-template-nav-mobile-icon"></span>
                    <ul class="docs-template-dropDown">
                        {{! Highlight the (active) category the document belongs to. }}
                        {{#each siteStructure}}
                            {{#ifEqual category ../document.category}}
                                <li class="docs-guidelines-nav{{category}} docs-template-category-active"><span aria-label="Currently on Category: {{category}}">{{category}}</span></li>
                            {{else}}
                                <li class="docs-guidelines-nav{{category}}"><a href="{{{getRelativeUrl categoryHref ../../document.url}}}" title="{{category}}">{{category}}</a></li>
                            {{/ifEqual}}
                        {{/each}}
                    </ul>
                </li>
            </ul>
        </nav>
    </div>

    <div class="docs-template-header">
        <nav class="docs-guidelines-tabs small-12 column show-for-medium-up">
            <ul class="inline-list">
            {{! Highlight the (active) category the document belongs to. }}
            {{#each siteStructure}}
                {{#ifEqual category ../document.category}}
                    <li class="docs-guidelines-nav{{category}} docs-template-category-active"><span aria-label="Currently on Category: {{category}}">{{category}}</span></li>
                {{else}}
                    <li class="docs-guidelines-nav{{category}}"><a href="{{{getRelativeUrl categoryHref ../../document.url}}}" title="{{category}}">{{category}}</a></li>
                {{/ifEqual}}
            {{/each}}
            </ul>
        </nav>
    </div>
</header>
